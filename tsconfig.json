{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src", // This must be specified if "paths" is.
    "paths": {
        "@app*": ["/*"],
        "@domain/*": ["domain/*"],
        "@presentation/*": ["presentation/*"],
        "@configs/*": ["configs/*"],
        "@navigations/*": ["navigations/*"],
        "@assets/*": ["assets/*"],
        "@data/*": ["data/*"]
    },
  }
}
